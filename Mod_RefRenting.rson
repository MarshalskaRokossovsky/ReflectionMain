          "Code":          
          [
              "if(!ShipIsTakeoff(Player())) {",
              "    for(i=GalaxyEvents()-1; i>=0; i=i-1) {",
              "        if(GalaxyEventDate(i) < CurTurn()) break;",
              "        if(GalaxyEventType(i)=='RuinsDestroyed') {",
              "            if(GalaxyEventData(i,0) != t_BK) continue;",
              "            BKcount = BKcount - 1;",
              "        }",
              "        if(GalaxyEventType(i)=='RuinsCreated') {",
              "            if(GalaxyEventData(i,0) != t_BK) continue;",
              "            BKcount = BKcount + 1;",
              "        }",
              "    }",
              "}",
              "for(y = 1; y < ArrayDim(timer); y = y + 1) {",
              "    if(timer[y] > 0) {",
              "        timer[y] = timer[y] - 1;",
              "    }",
              "    else if(bonkFlag[y] == 0) {",
              "        bonk[y] = 60;",
              "        if(PlayerMedPolicy() > 0) {",
              "            bonk[y] = bonk[y] + 30;",
              "            Ether(0,'',\"Рейнджер <Player>!<br>Уведомляем вас, что подошло время возвращать оборудование! У вас есть три месяца, чтобы прибыть в ближайшее отделение сети бизнес-центров и сдать оборудование.\");",
              "        }",
              "        else Ether(0,'',\"Рейнджер <Player>!<br>Уведомляем вас, что подошло время возвращать оборудование! У вас есть два месяца, чтобы прибыть в ближайшее отделение сети бизнес-центров и сдать оборудование.\");",
              "        bonkFlag[y] = 1;",
              "    }",
              "    if(bonk[y] != 0 && bonkFlag[y] == 1) bonk[y] = bonk[y] - 1;",
              "    if(bonk[y] == 0 && bonkFlag[y] == 1) {",
              "        Alarm = Alarm + 1;",
              "        Ether(0,'',\"Рейнджер <Player>!<br>Уведомляем вас, что вы просрочили сдачу оборудования на бизнес-центр! Если вы продолжите игнорировать наши требования, то наши контрмеры не заставят себя ждать!\");",
