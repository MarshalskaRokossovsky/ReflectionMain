          "Code.Type":  "Turn",
          "Code":          
          [
              "//Каждый год мы с друз... со скриптами идём проверять, КАК ТАМ домики",
              "if(CheckingCD != 0) CheckingCD = CheckingCD - 1;",
              "if(CheckingCD == 0) {",
              "    for(i=0;GalaxyStars()>i;i=i+1) {",
              "        if(StarOwner(GalaxyStar(i)) == 5) {",
              "            CountSys = CountSys + 1;",
              "            if(StarSeries(GalaxyStar(i)) == 0) CountKel = CountKel + 1;",
              "            if(StarSeries(GalaxyStar(i)) == 1) CountBlaz = CountBlaz + 1;",
              "            if(StarSeries(GalaxyStar(i)) == 2) CountTer = CountTer + 1;",
              "        }",
              "    }",
              "    CountKel = CountKel * 100;",
              "    if(CountKel / CountSys < 15) {",
              "        fot(j=0;GalaxyStars",
              "        for(i=0;7>i;i=i+1) {",
              "            int rand = Rnd(1,4);",
              "            if(rand == 1) SpawnExtraHeavyFleet(star);",
              "            if(rand == 2) SpawnHeavyFleet(star);",
              "            if(rand == 3) SpawnMissileFleet(star);",
              "            if(rand == 4) SpawnFastFleet(star);",
              "        }",
              "    }",
              "    CountKel = CountKel / 100;",
              "",
              "    CheckingCD = 365;",
              "}"          
          ]        
        }      
