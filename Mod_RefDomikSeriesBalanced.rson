          "Code":          
          [
              "function SpawnHeavyFleet(star) {",
              "    int cnty,i;",
              "    dword t_plan;",
              "",
              "    //Тяжёлый флот состоит из трёх-четырёх эквенторов и нескольких ургантов",
              "",
              "    //Сколько спавним эквенторов?",
              "    cnty = Rnd(2,4);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 1;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 1;",
              "",
              "    //Ищем планету для доминаторских утех с процессом спавна",
              "    for(i=0;StarPlanets(star)>i;i=i+1) {",
              "        if(PlanetOwner(StarPlanets(star,i)) == 6 continue;",
              "        t_plan = StarPlanets(star,i);",
              "        break;",
              "    }",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,1);",
              "    }",
              "",
              "    //Сколько спавним ургантов?",
              "    cnty = Rnd(4,7);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 1;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 2;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,2);",
              "    }",
              "}",
              "function SpawnRocketFleet(star) {",
              "    int cnty,i;",
              "    dword t_plan;",
              "",
              "    //Ракетный флот состоит из пяти-семи ургантов и десятка+ смершей",
              "",
              "    //Сколько спавним ургантов?",
              "    cnty = Rnd(5,7);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 1;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 2;",
              "",
              "    //Ищем планету для доминаторских утех с процессом спавна",
              "    for(i=0;StarPlanets(star)>i;i=i+1) {",
              "        if(PlanetOwner(StarPlanets(star,i)) == 6 continue;",
              "        t_plan = StarPlanets(star,i);",
              "        break;",
              "    }",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,2);",
              "    }",
              "",
              "    //Сколько спавним смершей?",
              "    cnty = Rnd(8,12);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 3;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 5;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,3);",
              "    }",
              "}",
              "function SpawnFastFleet(star) {",
              "    int cnty,i;",
              "    dword t_plan;",
              "",
              "    //Маневренный флот состоит из четырёх-девяти менков, 15+ штипов и нескольких клигов",
              "",
              "    //Сколько спавним менков?",
              "    cnty = Rnd(4,9);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 4;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 7;",
              "",
              "    //Ищем планету для доминаторских утех с процессом спавна",
              "    for(i=0;StarPlanets(star)>i;i=i+1) {",
              "        if(PlanetOwner(StarPlanets(star,i)) == 6 continue;",
              "        t_plan = StarPlanets(star,i);",
              "        break;",
              "    }",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,4);",
              "    }",
              "",
              "    //Сколько спавним штипов?",
              "    cnty = Rnd(15,22);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 6;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 10;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,5);",
              "    }",
              "",
              "    //Сколько спавним клигов?",
              "    cnty = Rnd(3,7);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 2;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 4;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,7);",
              "    }",
              "}",
              "function SpawnExtraHeavyFleet(star) {",
              "    int cnty,i;",
              "    dword t_plan;",
              "",
              "    //Сверхтяжёлый флот состоит из трёх берторов и нескольких эквенторов",
              "",
              "    //Сколько спавним эквенторов?",
              "    cnty = Rnd(7,11);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 2;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 3;",
              "",
              "    //Ищем планету для доминаторских утех с процессом спавна",
              "    for(i=0;StarPlanets(star)>i;i=i+1) {",
              "        if(PlanetOwner(StarPlanets(star,i)) == 6 continue;",
              "        t_plan = StarPlanets(star,i);",
              "        break;",
              "    }",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,1);",
              "    }",
              "",
              "    //Сколько спавним берторов?",
              "    cnty = 3;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 1;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        BuyDomikExtremal(t_plan,6);",
              "    }",
              "}"          
          ]        
        },
