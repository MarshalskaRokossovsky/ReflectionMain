              "AddDialogBlock(CT('FormRuins.Bridge.BridgeExit'),2);",
              "AddDialogInject('Dialog_Maps', '', \"Картографическая комиссия КВСМК\", 1);"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  50,
          "Pos.y":  10,
          "Parent":  -1,
          "#":  13,
          "Code.Type":  "Turn",
          "Code":          
          [
              "NoFlagy = 0;",
              "tempik = ListBox('Выберите систему из списка',NamesArray);",
              "if(tempik == -1) NoFlagy = 2;",
              "else if(tempik == 20) NoFlagy = 2;",
              "else {",
              "    tempik = tempik - 1;",
              "    if(SectorVisible(GalaxySectors(tempik))) NoFlagy = 1;",
              "    else if(!SectorVisible(GalaxySectors(tempik))) {",
              "        strSector = NamesArray[tempik];",
              "        if(ShipMoney(Player()) > 5000000) ein = ShipMoney(Player()) / 50000;",
              "        if(ShipMoney(Player()) < 5000000) ein = ShipMoney(Player()) / 30000;",
              "        if(ShipMoney(Player()) < 1000000) ein = ShipMoney(Player()) / 15000;",
              "        if(ShipMoney(Player()) < 300000) ein = ShipMoney(Player()) / 7500;",
              "        if(ShipMoney(Player()) < 100000) ein = ShipMoney(Player()) / 5000;",
              "        zwei = GalaxyTechLevel() * GalaxyDiffLevels(5) / 50;",
              "        money = 30 * ein * zwei * Rnd(80,120) / 100 + 5000;",
              "        if(ConNear(StarToCon(ShipStar(Player())),GalaxySectors(tempik))) int dab;",
              "        else money = money * 2;",
              "        if(ShipMoney(Player()) < money) AddDialogBlock('Да, пересылайте!',1);",
              "        else AddDialogBlock('Да, пересылайте!',0);",
              "    }",
              "}",
              "//SFT(NoFlagy);",
              "StartFlag = 1;"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  250,
          "Pos.y":  10,
          "Parent":  -1,
          "#":  16,
          "Code.Type":  "Turn",
          "Code":          
          [
              "ShipMoney(Player(),ShipMoney(Player()) - money);",
              "SectorVisible(GalaxySectors(tempik),1);",
              "Sound('Sound.Sell');",
              "StartFlag = 1;"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  50,
          "Pos.y":  -140,
          "Parent":  -1,
          "#":  31,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if(StartFlag == 1) temp_str = 'Вы вновь зашли на сайт Картографической Комиссии. Снова вас встретила приятная анимация перехода к чёрно-звёздному фону, и вы подумали, что не зря они ставят такие цены на звёздные карты - хотя бы смогли нанять хороших программистов и художников.';",
              "else temp_str = 'Этот сайт отличался от других. Несмотря на, походу, уходящие далеко в века рассказы о том, какие анахронизмы, ляпы, глюки и баги встречаются на сайтах государственных учреждений, этот сайт встретил приятной анимацией постепенного перехода к чёрно-звёздному фону.';"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -60,
          "Pos.y":  -90,
          "Parent":  -1,
          "#":  41,
          "Code.Type":  "Init",
          "Code":          
          [
              "int i;",
              "sectorsArr = newarray(1);",
              "for(i = 0; i < 19; i = i + 1) {",
              "    ArrayAdd(sectorsArr,GalaxySectors(i));",
              "}",
              "NamesArray = newarray(1);",
              "for(i = 0; i < 19; i = i + 1) {",
              "    ArrayAdd(NamesArray,Name(GalaxySectors(i)));",
              "}",
              "ArrayAdd(NamesArray,'Вернуться к выбору','Eexit');"          
          ]        
        }      
