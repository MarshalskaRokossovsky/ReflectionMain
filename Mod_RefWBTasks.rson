          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "if(GetShipRuins(Player())) {",
              "    if(ShipType(GetShipRuins(Player())) == 'WB') {",
              "        if(!DefSys) {",
              "            if(!DomiksDefeated()) {",
              "                AddDialogInject('WBTask',\"Нет ли у вас для меня какого особого задания?\",'',20);",
