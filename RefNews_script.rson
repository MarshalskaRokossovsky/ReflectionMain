          "Parent":  -1,
          "#":  89,
          "Code.Type":  "Turn",
          "Code":          
          [
              "//Любые празденства",
              "str temp_str1;",
              "if(!IsScriptActive('Mod_RefAltStartOnlyPirates')) {",
              "    if(FlagDomDef == 0) {",
              "        if(DomiksDefeated()) {",
              "            temp_str1 = Format(CT('CelebrationEvents.DomiksDefeated'),'#date#',GameDateTxtByTurn(CurTurn()));",
              "            AddPlanetNews(temp_str1);",
              "            NewsAdd(temp_str1);",
              "            Sound('Sound.NewMsg');",
              "            DomiksVictoryDate = CurTurn();",
              "            FlagDomDef = 1;",
              "        }",
              "    }",
              "    if(FlagNoDoms == 0) {",
              "        if(ControlledSystems(1) == 0 && DomiksDefeated()) {",
              "            AddPlanetNews(Format(CT('CelebrationEvents.GalaxyCleanedFromDomiks'),'#date#',GameDateTxtByTurn(CurTurn())));",
              "            FlagNoDoms = 1;",
              "        }",
              "    }",
              "}",
              "if(FlagNoCoal == 0) {",
              "    if(CoalitionDefeated()) {",
              "        CoalDeathDate = CurTurn();",
              "        FlagNoCoal = 1;",
              "    }",
              "}",
              "if(FlagNoPirates == 0) {",
              "    if(ControlledSystems(2) == 0 && PirateWin() == 3) {",
              "        AddPlanetNews(Format(CT('CelebrationEvents.GalaxyCleanedFromPirates'),'#date#',GameDateTxtByTurn(CurTurn())));",
              "        FlagNoPirates = 1;",
              "    }",
              "}",
              "if(FlagPiratesDef == 0) {",
              "    if(CoalitionDefeated()) {",
              "        ClanDeathDate = CurTurn();",
              "        FlagPiratesDef = 1;",
              "    }",
              "}",
              "//Сверху - победные ивенты. Снизу - годовщины победы",
              "dword tdwd;",
              "if(FlagDomDef == 1) {",
              "    tdwd = CurTurn() - DomiksVictoryDate;",
              "    if(tdwd % 365 == 0) {",
              "        if(!CoalitionDefeated()) {",
              "            AddPlanetNews(Format(CT('CelebrationEvents.CelebrateDomiksCoal'),'#count#',tdwd / 365));",
              "        }",
              "        else {",
              "            AddPlanetNews(Format(CT('CelebrationEvents.DomiksCelebrateDomiksClan'),'#count#',tdwd / 365));",
              "        }",
              "    }",
              "}",
              "if(FlagPiratesDef == 1) {",
              "    tdwd = CurTurn() - ClanDeathDate;",
              "    if(tdwd % 365 == 0) {",
              "        AddPlanetNews(Format(CT('CelebrationEvents.CelebratePiratesDeath'),'#count#',tdwd / 365));",
              "    }",
              "}",
              "if(FlagNoCoal == 1) {",
              "    tdwd = CurTurn() - CoalDeathDate;",
              "    if(tdwd % 365 == 0) {",
              "        AddPlanetNews(Format(CT('CelebrationEvents.CelebrateCoalitionDeath'),'#count#',tdwd / 365));",
              "    }",
              "}"          
          ]        
        },
