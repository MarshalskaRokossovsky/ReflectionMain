          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  280,
          "Pos.y":  -110,
          "Parent":  -1,
          "#":  22,
          "Code.Type":  "Turn",
          "Code":          
          [
              "int temp_money = GalaxyMoney(4,3) * 4;",
              "ShipMoney(Player(),ShipMoney(Player())+temp_money);",
              "Sound('Sound.QuestOk');",
              "",
              "AddItemToShip(Player(),CreateArt(t_ArtBio,Maloc));",
              "",
              "tstr1 = Format(CT('Script.Mod_MorphyRagobamToad.GovReward'),'money',temp_money,'planet',Name(GetShipPlanet(Player())),'star',Name(ShipStar(Player())));",
              "tstr1 = tstr1 + Format('<br>') + CT('Artefacts.ArtBio.Description');",
              "tstr2 = CT('Script.Mod_MorphyRagobamToad.PlayerRewardAnswer');",
              "StimAdded = 2;",
              "chibis = 0;"          
          ]        
