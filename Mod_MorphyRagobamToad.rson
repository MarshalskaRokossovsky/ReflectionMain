          "Pos.x":  -10,
          "Pos.y":  -90,
          "Parent":  -1,
          "#":  6,
          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "if(GetShipPlanet(Player())) {",
              "    if(StimAdded) {",
              "        if(IdToPlanet(PlanetID) == GetShipPlanet(Player())) {",
              "            if(!chibis) AddDialogInject('OnPlanet',CT('Script.Mod_MorphyRagobamToad.AddToGreeting'),CT('Script.Mod_MorphyRagobamToad.PlayerAsks'),10);",
              "            else AddDialogOverride('AfterQuest',99);",
              "        }",
              "    }",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -50,
          "Pos.y":  -130,
          "Parent":  -1,
          "#":  7,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if(ShipPilotRace(Player()) != Fei && ShipPilotRace(Player()) != Gaal) {",
              "    if(!StimAdded) {",
              "        if(ShipHealthFactor(Player(),19)) {",
              "            //Раз в пять ходов, чтобы не так сильно напрягало игру",
              "            if(CurTurn() % 5 == 0) {",
              "                if(~SearchForPlanet()) {",
              "                    PlanetID = SearchForPlanet();",
              "                    StimAdded = 1;",
              "                    Sound('Sound.NewMsg');",
              "                    NewsAdd(Format(CT('Script.Mod_MorphyRagobamToad.InvitingMessage'),'planet',Name(IdToPlanet(PlanetID)),'star',Name(PlanetToStar(IdToPlanet(PlanetID)))));",
              "                }",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "if(StimAdded) {",
              "    //Стимулятор помер - обнуляем",
              "    if(!ShipHealthFactor(Player(),19)) {",
              "        StimAdded = 0;",
              "    }",
              "",
              "    if(StimAdded == 1) {",
              "        //Проверяем, не захватили ли планету",
              "        if(PlanetID > 0) {",
              "            dword planet = IdToPlanet(PlanetID);",
              "    ",
              "            if(PlanetOwner(planet)) {",
              "                StimAdded = 0;",
              "            }",
              "            if(PlanetCustomFaction(planet) != '') {",
              "                StimAdded = 0;",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "if(GQuestStatus == 3) {",
              "    RelationToRanger(IdToPlanet(PlanetID),Player(),RelationToRanger(IdToPlanet(PlanetID),Player()) - 10);",
              "}",
              "else if(GQuestStatus == 2) {",
              "    FormChange('Gov');",
              "    chibis = 1;",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -10,
          "Pos.y":  -130,
          "Parent":  -1,
          "#":  8,
          "Code.Type":  "Init",
          "Code":          
          [
              "function SearchForPlanet() {",
              "    int i;",
              "    dword star;",
              "    dword planet = 0;",
              "",
              "    for(i=0;GalaxyStars()>i;i=i+1) {",
              "        star = StarNearbyStars(ShipStar(Player()),i);",
              "        //Отсеиваем ненужные системы",
              "        if(StarOwner(star)) continue;",
              "        if(StarCustomFaction(star) != '') continue;",
              "",
              "        planet = FindPlanet(star,'NotNone,NotMaloc,NotGaal,NotPeople,NotFei,NotPirateClan,NotKling');",
              "    }",
              "",
              "    if(planet > 0) result = Id(planet);",
              "    else result = -1;",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
