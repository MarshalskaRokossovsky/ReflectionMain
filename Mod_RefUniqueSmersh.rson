          "#":  20,
          "Total.Lines":  12,
          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "if(HullSpecial(Player()) == FindBonusByCustomTag('RefSmershMi') || HullSpecial(Player()) == FindBonusByCustomTag('RefSmershEn')) {",
