        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  60,
          "Pos.y":  70,
          "Parent":  -1,
          "#":  16,
          "Total.Lines":  7,
          "Code.Type":  "Turn",
          "Code":          
          [
              "ShipMoney(Player(),ShipMoney(Player()) - temp_int);",
              "temp_dword = CreateHull(1,1250,GalaxyTechLevel(),Fei);",
              "HullSeries(temp_dword,99);",
              "SpecialToEquipment(FindBonusByCustomTag('RefSmershMi'),temp_dword);",
              "AddItemToStorage(GetShipRuins(Player()),temp_dword);",
              "SmershBought = 1;",
              "Sound('Sound.Buy');"          
          ]        
        },
