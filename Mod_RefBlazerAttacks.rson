              "if(AttackCD == 0 && !DomiksDefeated(1) && AttackStage == 0) {",
              "    //Блазер живой? Значит, идём искать систему!",
              "    if(!ShipInStar(Blazer(),ShipStar(Player())) && StarOwner(ShipStar(Blazer())) == 1 && StarSeries(ShipStar(Blazer())) == 0 && !StarBattle(ShipStar(Blazer()))) {",
              "        StarForAttack = FindStarToAttack();",
              "        CurStar = ShipStar(Blazer());",
              "        AttackStage = 1;",
              "        SpawnFastFleet(CurStar);",
              "        SpawnHeavyFleet(CurStar);",
              "        ShipJoin(BlazerGroup,Blazer());",
              "    }",
              "}",
              "if(AttackCD != 0) AttackCD = AttackCD - 1;"          
          ]        
