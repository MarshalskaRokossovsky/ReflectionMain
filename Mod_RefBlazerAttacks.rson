          "Code.Type":  "Init",
          "Code":          
          [
              "//Функции для спавна свиты Блазера",
              "function SpawnHeavyFleet(star) {",
              "    int cnty,i;",
              "    dword t_plan;",
              "",
              "    //Тяжёлый флот состоит из трёх-четырёх эквенторов и нескольких ургантов",
              "",
              "    //Сколько спавним эквенторов?",
              "    cnty = Rnd(2,4);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 1;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 1;",
              "",
              "    //Ищем планету для доминаторских утех с процессом спавна",
              "    for(i=0;StarPlanets(star)>i;i=i+1) {",
              "        if(PlanetOwner(StarPlanets(star,i)) == 6) continue;",
              "        t_plan = StarPlanets(star,i);",
              "        break;",
              "    }",
              "    SFT('Heavy spawned');",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        ShipJoin(BlazersGuysGroup,BuyDomikExtremal(t_plan,1));",
              "        SFT('Joined-1');",
              "    }",
              "",
              "    //Сколько спавним ургантов?",
              "    cnty = Rnd(4,7);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 1;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 2;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        ShipJoin(BlazersGuysGroup,BuyDomikExtremal(t_plan,2));",
              "        SFT('Joined-2');",
              "    }",
              "}",
              "function SpawnFastFleet(star) {",
              "    int cnty,i;",
              "    dword t_plan;",
              "    SFT('Light spawned');",
              "",
              "    //Маневренный флот состоит из четырёх-девяти менков, 15+ штипов и нескольких клигов",
              "",
              "    //Сколько спавним менков?",
              "    cnty = Rnd(4,9);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 4;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 7;",
              "",
              "    //Ищем планету для доминаторских утех с процессом спавна",
              "    for(i=0;StarPlanets(star)>i;i=i+1) {",
              "        if(PlanetOwner(StarPlanets(star,i)) == 6) continue;",
              "        t_plan = StarPlanets(star,i);",
              "        break;",
              "    }",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        ShipJoin(BlazersGuysGroup,BuyDomikExtremal(t_plan,4));",
              "        SFT('Joined-3');",
              "    }",
              "",
              "    //Сколько спавним штипов?",
              "    cnty = Rnd(15,22);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 6;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 10;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        ShipJoin(BlazersGuysGroup,BuyDomikExtremal(t_plan,5));",
              "        SFT('Joined-4');",
              "    }",
              "",
              "    //Сколько спавним клигов?",
              "    cnty = Rnd(3,7);",
              "    if(GalaxyDiffLevels() > 200) cnty = cnty + 2;",
              "    if(GalaxyDiffLevels() > 350) cnty = cnty + 4;",
              "",
              "    //Начинаем спавнить",
              "    for(i=0;cnty>i;i=i+1){",
              "        //Не забываем их сразу закидывать в скриптовую группу",
              "        ShipJoin(BlazersGuysGroup,BuyDomikExtremal(t_plan,7));",
              "        SFT('Joined-4');",
              "    }",
              "}",
              "function FindStarToAttack() {",
              "    dword tstar;",
              "    int i;",
              "",
              "    for(i=0;GalaxyStars()>i;i=i+1) {",
              "        if(StarToCon(StarNearbyStars(ShipStar(Blazer()),i)) == StarToCon(ShipStatistic(Player(),10)) && CurTurn() < 1800) continue;",
              "        if(StarOwner(StarNearbyStars(ShipStar(Blazer()),i)) == 1 && StarSeries(StarNearbyStars(ShipStar(Blazer()),i)) == 0) continue;",
              "        if(Rnd(1,2) == 2) tstar = StarNearbyStars(ShipStar(Blazer()),i);",
              "        else continue;",
              "        SFT('Found-4');",
              "        break;",
              "    }",
              "",
              "    result = tstar;",
              "}",
              "if(ShipFindCustomShipInfoByType(Blazer(),'ShipTEmpImmortalInfo') != -1) ShipDeleteCustomShipInfo(Blazer(),'ShipTempImmortalInfo');"          
          ]        
        },
        {
          "Type":  "Top",
