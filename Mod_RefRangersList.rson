          "Parent":  -1,
          "#":  14,
          "Code.Type":  "Turn",
          "Code":          
          [
              "RangersList = \"Пилоты, которых вы можете нанять в данный момент времени:\";",
              "if(!GetGameOptions('FontDialog')) temp_str='--------------------------------';",
              "else if(GetGameOptions('FontDialog')==1) temp_str='---------------------------';",
              "else if(GetGameOptions('FontDialog')==2) temp_str='-----------------------------';",
              "else temp_str='---------------------------';",
              "RangersList = RangersList + CT('FormInfo.br') + temp_str + CT('FormInfo.br');",
              "//Ищем подходящие кандидатуры",
              "dword cur_star = ShipStar(Player());",
              "dword tsect = StarToCon(ShipStar(Player()));",
              "int cnt1=0;",
              "for(j=0;j<ConStars(tsect);j=j+1) {",
              "    for(i=0;i<StarShips(ConStar(tsect,j));i=i+1) {",
              "        dword cur_ship = StarShips(ConStar(tsect,j),i);",
              "        if(ShipType(Player()) != 'RangerPiratePlayer') {",
              "            if(ShipType(cur_ship) == 'Ranger' || ShipType(cur_ship) == 'FemRanger' || ShipType(cur_ship) == 'RangerExplorer' || ShipType(cur_ship) == 'Freelancer') {",
              "                if(ShipRank(Player()) >= ShipRank(cur_ship)) {",
              "                    if(cur_ship == Player()) continue;",
              "                    RangersList = RangersList + \"Имя пилота: \" + ShortName(cur_ship) + CT('FormInfo.br');",
              "                    RangersList = RangersList + \"Тип: \";",
              "                    if(ShipType(cur_ship) == 'Ranger') RangersList = RangersList + \"Рейнджер\" + CT('FormInfo.br');",
              "                    if(ShipType(cur_ship) == 'FemRanger') RangersList = RangersList + \"Спецагентка\" + CT('FormInfo.br');",
              "                    if(ShipType(cur_ship) == 'RangerExplorer') RangersList = RangersList + \"Рейнджер\" + CT('FormInfo.br');",
              "                    if(ShipType(cur_ship) == 'Freelancer') RangersList = RangersList + \"Фрилансер\" + CT('FormInfo.br');",
              "                    cnt1 = 1;",
              "                    guns = 0;",
              "                    for(k=0;k<5;k=k+1) {",
              "                        if(ShipEqInSlot(cur_ship,t_Weapon1,k)) guns = guns + 1;",
              "                    }",
              "                    if(ShipInHyperSpace(cur_ship)) RangersList = RangersList + \"Местоположение: в гиперпространстве\" + CT('FormInfo.br');",
              "                    else RangersList = RangersList + \"Местоположение: \" + Name(ShipStar(cur_ship)) + CT('FormInfo.br');",
              "                    RangersList = RangersList + \"Количество орудий: \" + guns + CT('FormInfo.br');",
              "                    RangersList = RangersList + \"Средний урон корабля за ход: \" + ShipAverageDamage(cur_ship) + CT('FormInfo.br');",
              "                    RangersList = RangersList + \"Сила защитного поля: \" + ShipProtectability(cur_ship) + CT('FormInfo.br');",
              "                    RangersList = RangersList + \"Восстановление дроидом: \" + ShipDroidRepair(cur_ship) + CT('FormInfo.br');",
              "                    RangersList = RangersList + temp_str + CT('FormInfo.br');",
              "                    CostIncrease = 1;",
              "                    AddDialogInject('DialNaim','','Связаться с '+Name(cur_ship),i);",
              "                }",
              "            }",
              "        }",
              "        else {",
              "            if(ShipType(cur_ship) == 'Pirate' || ShipType(cur_ship) == 'RangerPirate' || ShipType(cur_ship) == 'RangerPirateExplorer' || ShipType(cur_ship) == 'Freelancer') {",
              "                RangersList = RangersList + \"Имя пилота: \" + ShortName(cur_ship) + CT('FormInfo.br');",
              "                RangersList = RangersList + \"Тип: \";",
              "                if(ShipType(cur_ship) == 'Pirate') RangersList = RangersList + \"Пират\" + CT('FormInfo.br');",
              "                if(ShipType(cur_ship) == 'RangerPirate') RangersList = RangersList + \"Рейнджер-изменник\" + CT('FormInfo.br');",
              "                if(ShipType(cur_ship) == 'RangerPirateExplorer') RangersList = RangersList + \"Рейнджер-изменник\" + CT('FormInfo.br');",
              "                if(ShipType(cur_ship) == 'Freelancer') RangersList = RangersList + \"Фрилансер\" + CT('FormInfo.br');",
              "                cnt1 = 1;",
              "                guns = 0;",
              "                for(k=0;k<5;k=k+1) {",
              "                    if(ShipEqInSlot(cur_ship,t_Weapon1,k)) guns = guns + 1;",
              "                }",
              "                if(ShipInHyperSpace(cur_ship)) RangersList = RangersList + \"Местоположение: в гиперпространстве\" + CT('FormInfo.br');",
              "                else RangersList = RangersList + \"Местоположение: \" + Name(ShipStar(cur_ship)) + CT('FormInfo.br');",
              "                RangersList = RangersList + \"Количество орудий: \" + guns + CT('FormInfo.br');",
              "                RangersList = RangersList + \"Средний урон корабля за ход: \" + ShipAverageDamage(cur_ship) + CT('FormInfo.br');",
              "                RangersList = RangersList + \"Сила защитного поля: \" + ShipProtectability(cur_ship) + CT('FormInfo.br');",
              "                RangersList = RangersList + \"Восстановление дроидом: \" + ShipDroidRepair(cur_ship) + CT('FormInfo.br');",
              "                RangersList = RangersList + temp_str + CT('FormInfo.br');",
              "            }",
              "        }",
              "    }",
              "}",
              "//Если никого не нашли :(",
              "if(cnt1 != 1) RangersList = RangersList + 'Подходящих кандидатур в текущем секторе не обнаружено';"          
          ]        
        },
        {
