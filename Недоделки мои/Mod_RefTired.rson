          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "if(GetTalkShip() == 0) {",
              "    if(GetShipRuins(Player()) == t_MC) {",
              "        AddDialogInject('DialogHospital','Для всех рейнджеров можем оформить поездку в лечебный диспансер на планете-курорте по сниженной цене - всего <temp_str> cr. за месяц!','Поездка в диспансер?',1);",
              "    }",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  0,
          "Pos.y":  -130,
          "Parent":  -1,
          "#":  8,
          "Total.Lines":  68,
          "Code.Type":  "Turn",
          "Code":          
          [
              "int StFlag = 0;",
              "int StunFlag = 0;",
              "int Schit = 0;",
              "Ustal = Ustal + 2;",
              "if(HullDamage(Player()) > 0) Ustal = Ustal + 2;",
              "if(HullDamage(Player()) > 75) Ustal = Ustal + 10;",
              "if(OrderForsage(Player()) Ustal = Ustal + 5;",
              "if(ShipHealthFactor(Player(),1) > 0) Ustal = Ustal + 7;",
              "if(ShipHealthFactor(Player(),2) > 0) Ustal = Ustal + 3;",
              "if(ShipHealthFactor(Player(),3) > 0) Ustal = Ustal + 10;",
              "if(ShipHealthFactor(Player(),4) > 0) Ustal = Ustal + 7;",
              "if(ShipHealthFactor(Player(),5) > 0) Ustal = Ustal + 2;",
              "if(ShipHealthFactor(Player(),6) > 0) Ustal = Ustal + 25;",
              "if(ShipHealthFactor(Player(),7) > 0) Ustal = Ustal + 6;",
              "if(ShipHealthFactor(Player(),8) > 0) Ustal = Ustal + 9;",
              "if(ShipHealthFactor(Player(),9) > 0) Ustal = Ustal + 2;",
              "if(ShipHealthFactor(Player(),10) > 0) Ustal = Ustal + 1;",
              "if(ShipHealthFactor(Player(),11) > 0) Ustal = Ustal + 4;",
              "if(ShipHealthFactor(Player(),12) > 0) Ustal = Ustal + 3;",
              "//100% = 50000 очков усталости",
              "temp_str_BASED = 'Степень усталости: SU';",
              "if(Ustal > 2500) temp_str_NONBASED = Format(temp_str_BASED,'SU','вы слегка устали');",
              "if(Ustal > 15000) temp_str_NONBASED = Format(temp_str_BASED,'SU','вы устали');",
              "if(Ustal > 20000) {",
              "    temp_str_NONBASED = Format(temp_str_BASED,'SU','вы сильно истощены');",
              "    if(StFlag == 2) {",
              "        ShipSpecialBonuses(Player(),bonSkill1,ShipSpecialBonuses(Player(),bonSkill1) - 1);",
              "        ShipSpecialBonuses(Player(),bonSkill2,ShipSpecialBonuses(Player(),bonSkill2) - 1);",
              "        StFlag = 3;",
              "    }",
              "}",
              "if(Ustal > 27000) {",
              "    temp_str_NONBASED = Format(temp_str_BASED,'SU','вы в крайнем измождении');",
              "    if(StFlag == 1) {",
              "        ShipSpecialBonuses(Player(),bonSkill1,ShipSpecialBonuses(Player(),bonSkill1) - 1);",
              "        ShipSpecialBonuses(Player(),bonSkill2,ShipSpecialBonuses(Player(),bonSkill2) - 1);",
              "        ShipSpecialBonuses(Player(),bonSkill5,ShipSpecialBonuses(Player(),bonSkill5) - 1);",
              "        StFlag = 2;",
              "    }",
              "}",
              "if(Ustal > 38000) {",
              "    temp_str_NONBASED = Format(temp_str_BASED,'SU','вы едва ходите');",
              "    if(StFlag == 0) {",
              "        ShipSpecialBonuses(Player(),bonSkill1,ShipSpecialBonuses(Player(),bonSkill1) - 1);",
              "        ShipSpecialBonuses(Player(),bonSkill2,ShipSpecialBonuses(Player(),bonSkill2) - 2);",
              "        ShipSpecialBonuses(Player(),bonSkill4,ShipSpecialBonuses(Player(),bonSkill4) - 1);",
              "        ShipSpecialBonuses(Player(),bonSkill5,ShipSpecialBonuses(Player(),bonSkill5) - 1);",
              "        StFlag = 1;",
              "    }",
              "    if(Rnd(1,100) > 95) {",
              "        ShipAddCustomShipInfo(Player(),'ExpArts_ShipStunned');",
              "        StunFlag = 1;",
              "        Schit = Rnd(2,5);",
              "    }",
              "    if(StunFlag == 1 & Schit == 0) {",
              "        ShipDeleteCustomShipInfo(Player(),'ExpArts_ShipStunned');",
              "        Ether(8,'','Вы вели корабль, когда вдруг на вас накатилась тяжесть месяцев за штурвалом. Глаза закрылись, и вы погрузились в глубокий сон.<br>Наконец вы проснулись. Судя по бортовому компьютеру, прошло несколько дней. Походу, пора поскорее отложить все дела и хорошенько отдохнуть.');",
              "    }",
              "}",
              "if(Ustal < 2500) temp_str_NONBASED = Format(temp_str_BASED,'SU','вы в полном порядке');",
              "ShipCustomShipInfoDescription(Player(),'RefTireness',temp_str_NONBASED);",
              "//расчёт стоимости месяца в санаторее",
              "temp_str = GalaxyMoney(1) + 1100 + Rnd(10,300);",
              "//откат после пребывания в санаторее",
              "if(AfterMedicineFlag == 1) {",
              "    AfterMedicineFlag = 0;",
              "}",
              "if(Schit > 1) Schit = Schit - 1;"          
          ]        
        },
