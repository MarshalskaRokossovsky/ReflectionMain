          "Code.Type":  "Turn",
          "Code":          
          [
              "ShipAddCustomShipInfo(Player(),'RefZazhorFlag');",
              "ShipAddCustomShipInfo(Player(),'RefMainZazhorInfo');",
              "ShipSpecialBonuses(Player(),bonMass,ShipSpecialBonuses(Player(),bonMass) + 20);",
              "ShipSpecialBonuses(Player(),bonSkill6,ShipSpecialBonuses(Player(),bonSkill6) + 2);",
              "ShipSpecialBonuses(Player(),bonResistAsteroid,ShipSpecialBonuses(Player(),bonResistAsteroid) + 10);",
              "ShipSpecialBonuses(Player(),bonHull,ShipSpecialBonuses(Player(),bonHull) + 5);",
              "ShipSpecialBonuses(Player(),bonStimCapacity,ShipSpecialBonuses(Player(),bonStimCapacity) + 1);",
              "ShipSpecialBonuses(Player(),bonWMissile,ShipSpecialBonuses(Player(),bonWMissile) + 6);",
              "OneMoreKostyl = 1;",
              "Timer = 0;",
              "ShipGoods(Player(),t_Food,-15);"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  10,
          "Pos.y":  -90,
          "Parent":  -1,
          "#":  20,
          "Total.Lines":  6,
          "Code.Type":  "DialogBegin",
          "Code":          
          [
              "AddDialogInject('Zazhor','','А что касательно Зажора?',1);",
              "if(GoodsCount(Player(),t_Food) <= 14) Posit = 1;",
              "if(ShipFindCustomShipInfoByType(Player(),'RefZazhorFlag') > -1) Posit = 2;",
              "if(ShipFindCustomShipInfoByType(Player(),'RefZazhorFlag') == -1) {",
              "    if(GoodsCount(Player(),t_Food) >= 15) Posit = 3;",
              "}"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -30,
          "Pos.y":  -130,
          "Parent":  -1,
          "#":  23,
          "Total.Lines":  15,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if(OneMoreKostyl == 1) {",
              "    Timer = Timer + 1;",
              "    if(Timer == NextStop) {",
              "        ShipDeleteCustomShipInfo(Player(),'RefZazhorFlag');",
              "        ShipDeleteCustomShipInfo(Player(),'RefMainZazhorInfo');",
              "        ShipSpecialBonuses(Player(),bonMass,ShipSpecialBonuses(Player(),bonMass) - 20);",
              "        ShipSpecialBonuses(Player(),bonSkill6,ShipSpecialBonuses(Player(),bonSkill6) - 2);",
              "        ShipSpecialBonuses(Player(),bonResistAsteroid,ShipSpecialBonuses(Player(),bonResistAsteroid) - 10);",
              "        ShipSpecialBonuses(Player(),bonHull,ShipSpecialBonuses(Player(),bonHull) - 5);",
              "        ShipSpecialBonuses(Player(),bonStimCapacity,ShipSpecialBonuses(Player(),bonStimCapacity) - 1);",
              "        ShipSpecialBonuses(Player(),bonWMissile,ShipSpecialBonuses(Player(),bonWMissile) - 6);",
              "        OneMoreKostyl = 0;",
              "    }",
              "}",
              "if(Timer == 0) NextStop = Rnd(15,50);"          
          ]        
        }      
