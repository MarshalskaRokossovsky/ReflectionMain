              "    if(Name(GalaxyStar(i)) == CT('Star.04')) continue;",
              "    if(Name(GalaxyStar(i)) == CT('Star.Terron')) continue;",
              "    if(StarToCon(GalaxyStar(i)) == StarToCon(PlanetToStar(PlanetPirateClan()))) continue;",
              "    if(Rnd(1,100) > 90) {",
              "        if(t_Star1 == 0) t_Star1 = GalaxyStar(i);",
              "        if(t_Star2 == 0) t_Star2 = GalaxyStar(i);",
              "        if(t_Star3 == 0) t_Star3 = GalaxyStar(i);",
