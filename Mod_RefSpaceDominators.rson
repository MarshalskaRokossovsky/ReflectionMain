              "ItemSubrace(tdwd,PlayerRace);",
              "AddItemToShip(Player(),tdwd);",
              "ItemIsInUse(tdwd,Player(),1);",
              "tdwd = CreateEquipment(t_FuelTanks,Rnd(15,20),GalaxyTechLevel(),Kling);",
              "ItemSubrace(tdwd,PlayerRace);",
              "AddItemToShip(Player(),tdwd);",
              "ItemIsInUse(tdwd,Player(),1);",
