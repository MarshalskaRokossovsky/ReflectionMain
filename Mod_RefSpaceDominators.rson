              "    else if(rand == 3) tdwd = CreateEquipment(t_Weapon2,Rnd(25,35),GalaxyTechLevel(),Kling);",
              "    else tdwd = CreateEquipment(t_Weapon1,Rnd(15,23),GalaxyTechLevel(),Kling);",
              "    ItemSubrace(tdwd,PlayerRace);",
