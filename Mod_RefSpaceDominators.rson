              "ItemSubrace(tdwd,PlayerRace);",
              "AddItemToShip(Player(),tdwd);",
              "ItemIsInUse(tdwd,Player(),1);",
              "tdwd = CreateEquipment(t_RepairRobot,Rnd(20,25),GalaxyTechLevel(),Kling);",
              "ItemSubrace(tdwd,PlayerRace);",
              "AddItemToShip(Player(),tdwd);",
              "ItemIsInUse(tdwd,Player(),1);",
              "tdwd = CreateEquipment(t_Scaner,Rnd(15,20),GalaxyTechLevel(),Kling);",
