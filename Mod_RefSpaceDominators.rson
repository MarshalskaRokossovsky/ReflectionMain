              "    GetItemFromShip(Player(),tdwd);",
              "    FreeItem(tdwd);",
              "}",
              "if(ShipEqInSlot(Player(),t_FuelTanks)) {",
              "    tdwd = ShipEqInSlot(Player(),t_FuelTanks);",
              "    GetItemFromShip(Player(),tdwd);",
              "    FreeItem(tdwd);",
              "}",
              "if(ShipEqInSlot(Player(),t_Scaner)) {",
              "    tdwd = ShipEqInSlot(Player(),t_Scaner);",
              "    GetItemFromShip(Player(),tdwd);",
              "    FreeItem(tdwd);",
              "}",
              "if(ShipEqInSlot(Player(),t_DefGenerator)) {",
              "    tdwd = ShipEqInSlot(Player(),t_DefGenerator);",
              "    GetItemFromShip(Player(),tdwd);",
              "    FreeItem(tdwd);",
              "}",
              "if(ShipEqInSlot(Player(),t_RepairRobot)) {",
              "    tdwd = ShipEqInSlot(Player(),t_RepairRobot);",
              "    GetItemFromShip(Player(),tdwd);",
              "    FreeItem(tdwd);",
              "}",
              "if(ShipEqInSlot(Player(),t_CargoHook)) {",
              "    tdwd = ShipEqInSlot(Player(),t_CargoHook);",
              "    GetItemFromShip(Player(),tdwd);",
              "    FreeItem(tdwd);",
              "}",
              "for(i=0;ShipSlots(Player(),1)>i;i=i+1) {",
